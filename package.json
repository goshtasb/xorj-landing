{
  "name": "xorj-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:localhost": "cp .env.localhost .env.local && next dev --port 3002",
    "dev:production-localhost": "cp .env.production.localhost .env.local && next dev --port 3003",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "localhost:start": "./scripts/start-localhost.sh",
    "localhost:start:clean": "./scripts/start-localhost.sh --clean",
    "localhost:stop": "docker-compose -f docker-compose.localhost.yml down",
    "localhost:logs": "docker-compose -f docker-compose.localhost.yml logs -f",
    "production-localhost:start": "./scripts/start-production-localhost.sh",
    "production-localhost:stop": "pkill -f postgres; pkill -f redis-server",
    "worker": "npx ts-node src/lib/write-worker.ts",
    "worker:build": "npm run build && node dist/lib/write-worker.js",
    "test": "npm run lint && jest",
    "test:localhost": "npm run lint && BACKEND_MODE=live node run-e2e-simulation.js",
    "test:production-localhost": "npm run lint && BACKEND_MODE=production node run-production-e2e.js",
    "test:localhost:chaos": "npm run lint && BACKEND_MODE=live node run-staging-chaos-simulation.js"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/errors": "^2.3.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@supabase/supabase-js": "^2.57.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/ws": "^8.18.1",
    "autocannon": "^8.0.0",
    "bullmq": "^5.58.2",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "next": "^15.5.2",
    "p-ratelimit": "^1.0.1",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.2",
    "redis": "^5.8.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^29.7.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
